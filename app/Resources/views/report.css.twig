body, p, table th, table td{
    font-size: 16px;
    font-family: Solaimanlipi;
    color: black;
}
#pdf-report-page table th, #pdf-report-page table td {
    font-size: 14px;
    font-family: Solaimanlipi;
    color: black;
}
{#thead { display: table-header-group!important; }#}
{#tfoot { display: table-row!important; }#}
tr { page-break-inside: avoid!important; }

.filter-form div, .filter-form p, .filter-form label{
    font-size: 14px;
}

#pdf-report-page th {
    font-weight: normal;
    vertical-align: top;
}

.text-center, tr.text-center td, tr.text-center th {
    text-align: center
}

.table {
    border-collapse: collapse;
}

#pdf-report-page table.table, #pdf-report-page .table td, #pdf-report-page .table th {
    border: 1px solid black;
    padding: 3px;
}

.text-right {
    text-align: right
}

.text-left {
    text-align: left
}

.underline {
    text-decoration: underline
}

.border-bottom {
    border-bottom: 1px solid black
}

.bold, tr.bold td, tr.bold th {
    font-weight: bold !important;
}

.width-100 {
    width: 100%
}

.margin-bottom-10 {
    margin-bottom: 10px
}

.margin-bottom-5 {
    margin-bottom: 5px
}

.margin-bottom-20 {
    margin-bottom: 20px
}

.width-33 {
    width: 33%
}

.font-size-13 {
    font-size: 13px
}

.office-expense-body th, .office-expense-body td{
    font-size: 7px !important;
}

.details-size{
    width: 80px !important;
}
.code-no-size{
    width: 30px !important;
}
.amount-size{
    width: 45px !important;
}
.column160px{
    width: 160px;
}
.font-size-13 table,.font-size-13 thead,.font-size-13 tr,.font-size-13 td,.font-size-13 th,.font-size-13 tbody,.font-size-13 tfoot{
    font-size: 13px!important;
}
.color-red{
    color: red;
}