
{% extends '::base.html.twig' %}
{% block body %}
    <div class="container-fluid">
        <!-- BEGIN PAGE HEADER-->
        <div class="row-fluid">
            <div class="span12">
                <h3 class="page-title">
                    View Details
                </h3>
                <ul class="breadcrumb">
                    <li>
                        <i class="icon-home"></i>
                        <a href="{{ path('homepage') }}">Home</a>
                        <span class="icon-angle-right"></span>
                    </li>
                    <li>
                        <a href="{{ path('branch') }}">Branch</a>
                        <span class="icon-angle-right"></span>
                    </li>
                    <li><a href="#">View Details</a></li>
                    <li class="pull-right btn blue disabled mini">
                        <i class="fa fa-calendar"></i>
                        {{ "now"|date("d-F-Y") }}
                    </li>
                </ul>
            </div>
        </div>
        <!-- END PAGE HEADER-->
        <!-- BEGIN PAGE CONTENT-->
        <div class="row-fluid">
            <div class="span12">
               {# <div class="btn-group pull-right">
                    {% if entity.id > 0  %}
                        <a class="btn black" target="_blank" href="{{ path('branch_print',{'id': entity.id}) }}">Print</a>
                        <a class="btn yellow" target="_blank" href="{{ path('branch_pdf',{'id': entity.id}) }}">PDF</a>
                        <a class="btn blue" target="_blank" href="{{ path('branch_export',{'id': entity.id}) }}">Export</a>
                    {% else %}
                        <a class="btn purple" href="{{ path('branch_import') }}">Import</a>
                    {% endif %}
                </div>#}
                <div class="portlet box green tabbable">

                    <div class="portlet-title">
                        <div class="caption"><i class="icon-reorder"></i>Branch Information</div>
                    </div>

                    <div class="portlet-body">

                        <div class="tabbable portlet-tabs">
                            <ul class="nav nav-tabs">
                                <li class="active"><a  href="{{ path('branch_show',{'id':entity.id}) }}"><i class="icon-eye-open"></i>&nbsp;View</a></li>
                                <li class=""><a  href="{{ path('branch') }}"><i class="icon-th-list"></i>&nbsp;Lists</a></li>
                                <li><a  href="{{ path('branch_edit',{'id':entity.id}) }}"><i class="icon-edit"></i>&nbsp;Add/Edit</a></li>

                            </ul>
                            <div class="tab-content">
                                <div class="portlet-body form form-horizontal">
                                    {% include('flash-message.html.twig')  %}
                                    <div class="row-fluid">
                                        <div class="span12">
                                            <div class="control-group">
                                                <label class="control-label" ><strong>Branch Information</strong></label>
                                            </div>
                                        </div>
                                        <div class="span12">
                                            <div class="control-group">
                                                <label class="control-label" >Location</label>
                                                <div class="controls control-custom">{{ entity.name }}</div>
                                            </div>
                                        </div>
                                        <div class="span12">
                                            <div class="control-group">
                                                <label class="control-label" >Contact</label>
                                                <div class="controls control-custom">{{ entity.contactPerson }} ,{{ entity.designation }} </div>
                                            </div>
                                        </div>
                                        <div class="span12">
                                            <div class="control-group">
                                                <label class="control-label" >Mobile, Phone , Fax</label>
                                                <div class="controls control-custom">{{ entity.mobile }}, {{ entity.phone }}, {{ entity.fax }}</div>
                                            </div>
                                        </div>
                                        <div class="span12">
                                            <div class="control-group">
                                                <label class="control-label" >Email</label>
                                                <div class="controls control-custom">{{ entity.email }}</div>
                                            </div>
                                        </div>
                                        <div class="span11">
                                            <div class="control-group">
                                                <label class="control-label" >Address</label>
                                                <div class="controls control-custom">{{ entity.address }}</div>
                                            </div>
                                        </div>

                                        <div class="span11">
                                            <div class="control-group">
                                                <label class="control-label" >Google Map</label>
                                                <div class="controls control-custom">{{ entity.googleMap|raw }}</div>
                                            </div>
                                        </div>

                                        <div class="span12">
                                            <div class="control-group">
                                                <label class="control-label" >Status</label>
                                                <div class="controls control-custom">{% if entity.status == 1 %} Publish {% else %} Un-publish {% endif %}</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
{% endblock %}


