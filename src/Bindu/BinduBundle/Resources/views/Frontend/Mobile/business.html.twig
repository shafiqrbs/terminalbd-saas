{% extends ':bindu/mobile:base.html.twig' %}
{% block body %}
    <div class="bannerPane bannerPane-business-directory">
        <div class="overlay"></div>
        <div class="s-banner-content">
            <div><img width="100" src="{{ asset('assets/frontend/img/logo.png') }}" /></div>
            <div class="page-title"> E-Business Directory</div>
        </div>
    </div>
    <div id="content">
        <div id="directory">
            {% render controller("BinduBundle:Widget:business", {'directory':directory}) %}
            <div class="clear"></div>
        </div>
    </div>
{% endblock %}