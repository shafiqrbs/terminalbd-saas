        .tooltip-arrow{
            position:absolute;
            width:0;
            height:0;
        }
        .timeline > li.timeline-inverted{
            float: right;
            clear: right;
            margin-top: 30px;
            margin-bottom: 30px;
        }
        .timeline > li:nth-child(2){
            margin-top: 60px;
        }
        .timeline > li.timeline-inverted > .timeline-badge{
            left: -12px;
        }

        @media (max-width: 767px) {
            ul.timeline:before {
                left: 40px;
            }

            ul.timeline > li {
                margin-bottom: 20px;
                position: relative;
                width:100%;
                float: left;
                clear: left;
            }
            ul.timeline > li > .timeline-panel {
                width: calc(100% - 90px);
                width: -moz-calc(100% - 90px);
                width: -webkit-calc(100% - 90px);
            }

            ul.timeline > li > .timeline-badge {
                left: 28px;
                margin-left: 0;
                top: 16px;
            }

            ul.timeline > li > .timeline-panel {
                float: right;
            }

            ul.timeline > li > .timeline-panel:before {
                border-left-width: 0;
                border-right-width: 15px;
                left: -15px;
                right: auto;
            }

            ul.timeline > li > .timeline-panel:after {
                border-left-width: 0;
                border-right-width: 14px;
                left: -14px;
                right: auto;
            }

            .timeline > li.timeline-inverted{
                float: left;
                clear: left;
                margin-top: 30px;
                margin-bottom: 30px;
            }

            .timeline > li.timeline-inverted > .timeline-badge{
                left: 28px;
            }
        }

