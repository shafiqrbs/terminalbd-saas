                                <img class="img" srcset="{{ web_image(product.webPath).resize(358,358) }},
                 {{ web_image(product.webPath).resize(358,358) }} 1.5x,
                 {{ web_image(product.webPath).resize(358,358) }} 2x"
                                     src="{{ web_image(product.webPath).resize(358,358) }}"
