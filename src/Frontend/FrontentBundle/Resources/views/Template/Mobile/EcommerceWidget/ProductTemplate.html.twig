                            </picture>
                        {% else %}
                            <img class="img-responsive" alt="{{ product.name }}" src="{{ image(globalOption.ecommerceConfig.webPath).resize(400,320) }}"/>
                        {% endif %}
                    </a>
                </div>
                <div data-match-height="item-product-height" class="card-content" >
                    {% if product.category is not empty and  globalOption.ecommerceConfig.showCategory == 1 %}
                        <span class="category-name">
                                                        <a class="category-url" href="{{ path(globalOption.subDomain ~ '_webservice_product_category',{'category':product.category.slug }) }}">{{ product.category.name }}</a>
                                                    </span>
                    {% endif %}
                    <h3 class="title">
                        {% if globalOption.ecommerceConfig.productDetails == 1 %}
                            <a class="product-url" href="{{ path(globalOption.subDomain ~ '_webservice_product_details',{'item':product.slug }) }}" >{{ product.productName }}</a>
                        {% else %}
                            <a class="item-preview product-url" data-url="{{ path(globalOption.subDomain ~ '_webservice_product_modal',{'item':product.id }) }}" href="javascript:">{{ product.productName }}</a>
                        {% endif %}
